l_events(
happens(newbelief(B), T1, T3),
[
holds(belief(B,X,Y),T1),
increment(X,X1),
holds(not(tree(X1,Y)),T1),
happens(del_belief(B,X,Y), T1, T2),
happens(gen_belief(B,X1,Y), T2, T3)
]

).

l_events(
happens(newbelief(B), T1, T3),
[
holds(belief(B,X,Y),T1),
increment(Y,Y1),
holds(not(tree(X,Y1)),T1),
happens(del_belief(B,X,Y), T1, T2),
happens(gen_belief(B,X,Y1), T2, T3)
]

).





